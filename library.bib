@article{Glynn2010,
abstract = {Ecological inference is a problem of partial identification, and therefore reliable precise conclusions are rarely possible without the collection of individual level (identifying) data. Without such data, sensitivity analyses provide the only recourse. In this paper we review and critique approaches to ecological inference in the social sciences, and describe in detail hierarchical models, which allow both sensitivity analysis and the incorporation of individual level data into an ecological analysis. A crucial element of a sensitivity analysis in such models is prior specification, and we detail how this may be carried out. Furthermore, we demonstrate how the inclusion of a small amount of individual level data can dramatically improve the properties of such estimates.},
author = {Glynn, Adam and Wakefield, Jon},
doi = {10.1016/j.stamet.2009.09.003},
file = {:Users/phil/Documents/Mendeley Desktop/Glynn, Wakefield - 2010 - Ecological Inference in the Social Sciences.pdf:pdf},
issn = {1572-3127},
journal = {Statistical methodology},
keywords = {ecological inference},
month = may,
number = {3},
pages = {307--322},
pmid = {20563299},
publisher = {Elsevier B.V.},
title = {{Ecological Inference in the Social Sciences.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2885825\&tool=pmcentrez\&rendertype=abstract},
volume = {7},
year = {2010}
}
@article{Liu2013893,
abstract = {The literature of data envelopment analysis (DEA) encompasses many surveys, yet all either emphasize methodologies or do not make a distinction between methodological and application papers. This study is the first literature survey that focuses on DEA applications, covering DEA papers published in journals indexed by the Web of Science database from 1978 through August 2010. The results show that on the whole around two-thirds (63.6\%) of DEA papers embed empirical data, while the remaining one-third are purely-methodological. Purely-methodological articles dominated the first 20 years of DEA development, but the accumulated number of application-embedded papers caught up to purely-methodological papers in 1999. Among the multifaceted applications, the top-five industries addressed are: banking, health care, agriculture and farm, transportation, and education. The applications that have the highest growth momentum recently are energy and environment as well as finance. In addition to the basic statistics, we uncover the development trajectory in each application area through the main path analysis. An observation from these works suggests that the two-step contextual analysis and network DEA are the recent trends across applications and that the two-step contextual analysis is the prevailing approach.},
author = {Liu, John S. and Lu, Louis Y.Y. Y and Lu, Wen-Min and Lin, Bruce J.Y. Y},
doi = {10.1016/j.omega.2012.11.004},
file = {:Users/phil/Documents/Mendeley Desktop/Liu et al. - 2013 - A survey of DEA applications.pdf:pdf},
issn = {0305-0483},
journal = {Omega},
keywords = {Citation analysis,Literature survey,Main path analysis,data envelopment analysis},
month = oct,
number = {5},
pages = {893--902},
publisher = {Elsevier},
title = {{A survey of DEA applications}},
url = {http://www.sciencedirect.com/science/article/pii/S0305048312002186 http://dx.doi.org/10.1016/j.omega.2012.11.004 http://linkinghub.elsevier.com/retrieve/pii/S0305048312002186},
volume = {41},
year = {2013}
}
